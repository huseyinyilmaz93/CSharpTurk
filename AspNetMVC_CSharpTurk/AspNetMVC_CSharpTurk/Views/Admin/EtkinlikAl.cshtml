
@{
    ViewBag.Title = "EtkinlikAl";
}
<script src="~/Scripts/angular.js"></script>
<script src="~/Scripts/angular.min.js"></script>

<script>
    var module = angular.module('angular');

    module.controller('EtkinlikController', ['$scope', '$http', function ($scope, $http) {
        $scope.veri = {};
        $scope.Calis = function () {
            $http.get('https://etkinlik.io/api/v1/etkinlikler?kategoriId=456', {
                headers: [
                    'X-ETKINLIK-TOKEN:7fb992c135adff01cdc6c33f9903d064'
                ]
            }).success(function (data, status, headers, config) {
                $scope.veri = data;
            }).error(function (data, status, headers, config) {

            })
        }
    }]);
</script>

<div ng-controller="EtkinlikController">
    <form ng-submit="Calis()" ng-">
        <input type="text" />
        <input type="submit" />
    </form>
    {{veri}}
</div>